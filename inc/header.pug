                        </svg>

                    span.text История заказов

            .logo
                a(href="#")
                    img(src="img/logo.png")

            .currency
                -var currency=['','BYN','RUB','USD','EUR'];
                -var currency2=['','Беллорусский рубль','Российский рубль','Доллар США','Евро'];
                span Валюта:
                ul.currency_list
                    li
                        button.dropdown_toggle.currency_current
                            picture
                                img(src="img/currency/1.png")
                            span.text BYN
                        ul.dropdown_menu
                            -for(var c=1;c<=4;c++)
                                li.currency_item

                                    input(type="radio" name="currency" id="currency_"+c+"")
                                    label(for="currency_"+c+"")
                                        picture
                                            img(src="img/currency/"+c+".png")
                                        .text=currency[c]
                                            span=currency2[c]


            .cart
                .icon
                    img(src="img/cart.svg")
                    span.count 2
        .h_bottom_row
            .main_menu
                .mm_close
                    <svg width="23" height="23" viewBox="0 0 23 23" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M2 2L21 21" stroke="#898989" stroke-width="3" stroke-linecap="round"/>
                    <path d="M21 2L2 21" stroke="#898989" stroke-width="3" stroke-linecap="round"/>
                    </svg>

                .main_menu_container
                    .mm_header
                        .mm_toggler.mm_account
                            picture
                                img(src="img/user.svg")
                            span Регистрация/Вход
                        .mm_slide
                            .back
                                <svg width="24" height="22" viewBox="0 0 24 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M2 11H22M2 11L11 2M2 11L11 20" stroke="#898989" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
                                </svg>
                            .mm_slide_body.pt_0
                                ul.tabs
                                    li.act
                                        span Регистрация
                                    li
                                        span Вход
                                .tabs_content
                                    .tabs_content_item.active
                                        .mm_reg
                                            .social_reg
                                                span Через соц. сеть:
                                                ul.social_list
                                                    -for(var s=1;s<=5;s++)
                                                        li
                                                            a(href="#")
                                                                img(src="img/social/"+s+".png")
                                            form.custom_form
                                                .field
                                                    label Имя
                                                    input.form_control(type="text")
                                                .field
                                                    label Номер телефона
                                                    input.form_control(type="text")
                                                .field
                                                    label E-mail
                                                    input.form_control(type="text")
                                                .field
                                                    label Пароль
                                                    input.form_control(type="text")
                                                .field
                                                    button.btn_default(type="submit") Продолжить

                                    .tabs_content_item
                                        .mm_auth
                                            form.custom_form
                                                .field
                                                    label Логин
                                                    input.form_control(type="text")
                                                .field
                                                    label Пароль
                                                    input.form_control(type="text")
                                                .field
                                                    button.btn_default(type="submit") Войти





                    ul.nav
                        for mm_item, index in main_menu
                            li
                                a(href="#")=mm_item
                    .mm_footer
                        .history_orders
                            a(href="#")
                                picture
                                    <svg width="28" height="20" viewBox="0 0 28 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M2.06198 14.0206C2.94244 7.96043 8.2394 3.40466 13.7778 3.29112C19.0889 3.19177 24.8261 7.40692 25.948 14.0206C26.2462 14.0206 26.5444 14.0206 26.8427 14.0206C27.8225 14.049 28.2912 14.8154 27.8083 15.6811C26.658 17.7674 24.9113 19.1441 22.5824 19.7118C21.9291 19.8679 21.2333 19.9672 20.5516 19.9672C16.1919 19.9956 11.8464 20.024 7.48675 19.9672C4.29153 19.9246 1.82056 18.5338 0.244255 15.6953C-0.309583 14.7018 0.102245 14.049 1.23832 14.0348C1.50814 14.0206 1.76376 14.0206 2.06198 14.0206ZM4.16372 13.9638C10.753 13.9638 17.2996 13.9638 23.8321 13.9638C23.5764 10.5718 19.657 5.26386 13.863 5.30644C8.353 5.34902 4.34833 10.6002 4.16372 13.9638ZM2.91404 15.9933C4.20632 17.3132 5.65482 17.9661 7.31633 17.9803C11.7612 18.0229 16.2203 18.0229 20.6652 17.9803C22.341 17.9661 23.8037 17.3132 25.0533 15.9933C17.6404 15.9933 10.3411 15.9933 2.91404 15.9933Z" fill="#333333"/>
                                    <path d="M13.9623 1.97123C13.6499 1.97123 13.3517 1.97123 13.0393 1.97123C12.4144 1.97123 12.0594 1.68739 12.031 1.04873C12.0026 0.410067 12.3292 0.0552567 12.9541 0.0268718C13.6357 -0.0157055 14.3316 -0.00151304 15.0132 0.0268718C15.6238 0.0552567 15.9647 0.395875 15.9505 1.03453C15.9363 1.64481 15.6096 1.95704 14.9564 1.97123C14.6298 1.98543 14.289 1.97123 13.9623 1.97123Z" fill="#333333"/>
                                    </svg>

                                span.text История заказов




                        .currency
                            -var currency = ['', 'BYN', 'RUB', 'USD', 'EUR'];
                            -var currency2 = ['', 'Беллорусский рубль', 'Российский рубль', 'Доллар США', 'Евро'];
                            span Валюта:
                            ul.currency_list
                                li
                                    .mm_toggler.currency_current
                                        picture
                                            img(src="img/currency/1.png")
                                        span.text BYN
                                    .mm_slide
                                        .back
                                            <svg width="24" height="22" viewBox="0 0 24 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M2 11H22M2 11L11 2M2 11L11 20" stroke="#898989" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
                                            </svg>

                                        .mm_slide_body
                                            ul
                                                -for (var c = 1; c <= 4; c++)
                                                    li.currency_item
                                                        input(type="radio" name="currency" id="currency_1" + c + "")
                                                        label(for="currency_1" + c + "")
                                                            picture
                                                                img(src="img/currency/" + c + ".png")
                                                            .text=currency[c]
                                                                span=currency2[c]


            .m_right
                .account
                    ul.account_menu
                        li
                            button.dropdown_toggle
                                .icon
                                    img(src="img/user.svg")
                            ul.dropdown_menu
                                li
                                    a(href="#") Регистрация
                                li
                                    a(href="#") Вход
                a.cart(href="#")
                    .icon
                        img(src="img/cart.svg")
                        span.count 2

